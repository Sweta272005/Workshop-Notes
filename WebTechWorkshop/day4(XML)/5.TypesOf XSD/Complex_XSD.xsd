<!-- 2. Complex Type Element

Used when an element has child elements or attributes.

-->
<xs:element name="student">
  <xs:complexType>
    <xs:sequence>
      <xs:element name="name" type="xs:string"/>
      <xs:element name="age" type="xs:integer"/>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- example in xml -->

